{
    "default": true,
    "MD003": {
        "style": "atx"
    },
    "MD007": {
        "indent": 4
    },
    "MD009": {
        "br_spaces": 2
    },
    "MD012": {
        "maximum": 2
    },
    "MD013": false,
    "MD022": {
        "lines_above": 1,
        "lines_below": 1
    },
    "MD024": {
        "allow_different_nesting": true,
        "siblings_only": true
    },
    "MD025": {
        "front_matter_title": "^\\s*title\\s*[:=]"
    },
    "MD029": {
        "style": "ordered"
    },
    "MD030": {
        "ul_single": 1,
        "ul_multi": 1,
        "ol_single": 1,
        "ol_multi": 1
    },
    "MD033": {
        "allowed_elements": [
            "a",
            "img",
            "br",
            "details",
            "summary",
            "kbd",
            "sup",
            "sub",
            "mark",
            "div",
            "span"
        ]
    },
    "MD034": false,
    "MD035": {
        "style": "---"
    },
    "MD040": {
        "allowed_languages": [
            "",
            "text",
            "bash",
            "sh",
            "shell",
            "zsh",
            "fish",
            "csh",
            "json",
            "javascript",
            "js",
            "typescript",
            "ts",
            "python",
            "py",
            "yaml",
            "yml",
            "markdown",
            "md",
            "html",
            "css",
            "scss",
            "powershell",
            "ps1",
            "sql",
            "dockerfile",
            "ini",
            "toml",
            "xml",
            "csv",
            "plaintext",
            "console",
            "diff",
            "git",
            "ignore"
        ],
        "language_only": true
    },
    "MD041": {
        "front_matter_title": "^\\s*title\\s*[:=]"
    },
    "MD046": {
        "style": "fenced"
    },
    "MD048": {
        "style": "backtick"
    },
    "MD049": {
        "style": "underscore"
    },
    "MD050": {
        "style": "asterisk"
    }
}