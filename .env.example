# Environment variables template from PR #5
# Copy to .env and fill with your actual values

# Google Sheets Integration
GOOGLE_SHEETS_CREDS_PATH=/path/to/service_account.json
AUX_SHEET_ID=your_sheet_id_here
DESEMBOLSOS_RANGE=DESEMBOLSOS!A1:AV5000
DATA_SHEET_NAME=Data

# AI Service Integration
OPENAI_API_KEY=your_openai_key_here
GOOGLE_GEMINI_API_KEY=your_gemini_key_here

# Figma Integration
FIGMA_TOKEN=your_figma_token_here
FIGMA_FILE_ID=your_figma_file_id_here

# HubSpot Integration
HUBSPOT_API_KEY=your_hubspot_key_here

# SonarQube Configuration
SONAR_TOKEN=your_sonar_token_here
SONAR_HOST_URL=https://your-sonarqube-server.com
